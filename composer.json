{
    "name": "bgustyp/laravel-zmq",
    "description": "A ZeroMQ broadcast driver for Laravel 5-12 with DEALER socket support",
    "license": "MIT",
    "keywords": ["laravel", "zmq", "zeromq", "broadcasting", "dealer", "router", "pubsub"],
    "authors": [
        {
            "name": "pelim",
            "email": "peter.limbach@gmail.com"
        },
        {
            "name": "bgustyp",
            "email": "bgustyp@mail.com"
        }
    ],
    "require": {
        "ext-zmq": "*",
        "php": "^8.1",
        "illuminate/broadcasting": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/contracts": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/http": "^5.5|^6.0|^7.0|^8.0|^9.0|^10.0|^11.0|^12.0"
    },
    "autoload": {
        "psr-4": {
            "Bgustyp\\LaravelZmq\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bgustyp\\LaravelZmq\\ZmqServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": false
}
